<% content_for :title, " - Cadastro" %>

<div class="contentfull">
<h2>Cadastre-se</h2>

<% semantic_form_for @user do |form| %>
  <%= form.error_messages %>
  <%= render :partial => "/users/inputs", :locals => { :form => form, :op => "add" } %>
  <% form.buttons do %>
    <%= form.commit_button "Cadastrar" %>
  <% end %>
<% end %>
</div>
